&models {
	porbeagle: porbeagle {
		powerd-prefs = "porbeagle";
		test-label = "porbeagle";
		audio {
			main {
				audio-type = <&audio_type_1mic>;
				cras-config-dir = "astronaut";
			};
		};
		thermal {
			/* Could also point to astronaut/dptf.dv */
			dptf-dv = "porbeagle/dptf.dv";
		};
		power {
			touchpad-wakeup = "1";
		};
	};
};
