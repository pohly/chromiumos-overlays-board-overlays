CHASSIS="$(mosys platform chassis)"
CHASSIS_ID="HANA"

if [ "${CHASSIS}" = "MAPLE14" ]; then
	CHASSIS_ID="MAPLE14"
fi

echo "/etc/cras/${CHASSIS_ID}"
